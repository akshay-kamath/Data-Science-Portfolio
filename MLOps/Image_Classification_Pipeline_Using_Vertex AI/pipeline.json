    "deploymentSpec": {
      "executors": {
        "exec-automl-image-training-job": {
          "container": {
            "args": [
              "--init.project",
              "{{$.inputs.parameters['project']}}",
              "--init.location",
              "{{$.inputs.parameters['location']}}",
              "--init.display_name",
              "{{$.inputs.parameters['display_name']}}",
              "--init.prediction_type",
              "{{$.inputs.parameters['prediction_type']}}",
              "--init.multi_label",
